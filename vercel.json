{
    "version": 2,
    "builds": [
      {
        "src": "api/server.py",
        "use": "@vercel/python"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/log",
        "dest": "api/server.py"
      },
      {
        "src": "/logs",
        "dest": "api/server.py"
      }
    ]
  }
  